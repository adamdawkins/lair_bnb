<div id="<%= dom_id property %>">
  <p>
    <strong>Name:</strong>
    <%= property.name %>
  </p>

  <%= image_tag property.image %>

  <p>
    <strong>Description:</strong>
    <%= property.description %>
  </p>

  <p>
    <strong>User:</strong>
    <%= property.user.email %>
  </p>

  <p>
    <strong>Lat:</strong>
    <%= property.lat %>
  </p>

  <p>
    <strong>Lon:</strong>
    <%= property.lon %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= property.status %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= property.country %>
  </p>

</div>
